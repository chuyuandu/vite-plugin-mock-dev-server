{
  "name": "vite-plugin-mock-dev-server",
  "version": "1.1.16",
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin",
    "mock",
    "mock-server"
  ],
  "homepage": "https://vite-plugin-mock-dev-server.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pengzhanbo/vite-plugin-mock-dev-server"
  },
  "license": "MIT",
  "author": "pengzhanbo <q942450674@outlook.com> (https://github.com/pengzhanbo)",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "DEBUG=vite:plugin-mock-dev-server vite example --config ./example/vite.config.ts",
    "example:build": "DEBUG=vite:plugin-mock-dev-server vite build example --config ./example/vite.config.ts",
    "docs:build": "vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "bumpp package.json --execute=\"pnpm release:changelog\" --commit --all --push --tag && pnpm publish --access public"
  },
  "prettier": "@pengzhanbo/prettier-config",
  "dependencies": {
    "chokidar": "^3.5.3",
    "co-body": "^6.1.0",
    "cookies": "^0.8.0",
    "debug": "^4.3.4",
    "esbuild": "^0.18.4",
    "fast-glob": "^3.2.12",
    "formidable": "^3.4.0",
    "http-status": "^1.6.2",
    "is-core-module": "^2.12.1",
    "json5": "^2.2.3",
    "lodash.sortby": "^4.7.0",
    "mime-types": "^2.1.35",
    "path-to-regexp": "^6.2.1",
    "picocolors": "^1.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@pengzhanbo/eslint-config": "^0.3.14",
    "@pengzhanbo/prettier-config": "^0.3.14",
    "@types/co-body": "^6.1.0",
    "@types/cookies": "^0.7.7",
    "@types/debug": "^4.1.8",
    "@types/formidable": "^2.0.6",
    "@types/is-core-module": "^2.2.0",
    "@types/lodash.sortby": "^4.7.7",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.16.18",
    "@types/ws": "^8.5.5",
    "bumpp": "^9.1.1",
    "conventional-changelog-cli": "^3.0.0",
    "eslint": "^8.43.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.8",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitepress": "1.0.0-beta.2"
  },
  "peerDependencies": {
    "vite": ">=3.0.0"
  },
  "packageManager": "pnpm@8.5.0",
  "engines": {
    "node": "^14.18.0 || >=16"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
