{
  "name": "vite-plugin-mock-dev-server",
  "version": "0.3.13",
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin",
    "mock",
    "mock-server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pengzhanbo/vite-plugin-mock-dev-server"
  },
  "homepage": "https://vite-plugin-mock-dev-server.netlify.app",
  "author": "pengzhanbo <q942450674@outlook.com> (https://github.com/pengzhanbo)",
  "license": "GPL-3.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "DEBUG=vite:plugin-mock-dev-server vite example --config ./example/vite.config.ts",
    "build": "tsup",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp package.json --commit --push --tag && pnpm publish --access public"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "sourcemap": false,
    "dts": true,
    "splitting": false,
    "clean": true,
    "format": [
      "esm",
      "cjs"
    ]
  },
  "prettier": "@pengzhanbo/prettier-config",
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "chokidar": "^3.5.3",
    "co-body": "^6.1.0",
    "debug": "^4.3.4",
    "esbuild": "^0.16.9",
    "fast-glob": "^3.2.12",
    "formidable": "^2.1.1",
    "json5": "^2.2.2",
    "path-to-regexp": "^6.2.1"
  },
  "devDependencies": {
    "@pengzhanbo/eslint-config-ts": "^0.2.10",
    "@pengzhanbo/prettier-config": "^0.2.10",
    "@types/co-body": "^6.1.0",
    "@types/debug": "^4.1.7",
    "@types/formidable": "^2.0.5",
    "@types/node": "^18.11.7",
    "bumpp": "^8.2.1",
    "eslint": "^8.30.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.2",
    "vite-plugin-mock-dev-server": "file:",
    "vitepress": "1.0.0-alpha.32",
    "vue": "^3.2.45"
  },
  "peerDependencies": {
    "vite": ">=3.0.0"
  },
  "packageManager": "pnpm@7.18.2",
  "engines": {
    "node": "^14.18.0 || >=16"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
